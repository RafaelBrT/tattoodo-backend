generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement()) // Chave prim√°ria
  email     String   @unique
  name      String
  password  String
  role      Int      @default(1)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
